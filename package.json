{
  "author": "ismay",
  "description": "Source for ismaywolff.nl",
  "name": "ismaywolff.nl",
  "version": "1.0.0",
  "scripts": {
    "deploy": "npm-run-all deploy:build deploy:upload",
    "deploy:build": "node build.js",
    "deploy:upload": "surge --project ./dist",
    "start": "npm-run-all --parallel develop:*",
    "develop:serve": "browser-sync start --server '.tmp' --files '.tmp/**'",
    "develop:watch": "node build.js --development"
  },
  "devDependencies": {
    "browser-sync": "^2.14.0",
    "metalsmith": "^2.2.0",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-better-excerpts": "github:superwolff/metalsmith-better-excerpts#add-pattern-option",
    "metalsmith-cloudinary": "^0.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-define": "^2.0.1",
    "metalsmith-filenames": "^1.0.0",
    "metalsmith-if": "^0.1.1",
    "metalsmith-in-place": "^1.4.4",
    "metalsmith-mapsite": "^1.0.4",
    "metalsmith-robots": "^1.0.1",
    "metalsmith-watch": "^1.0.3",
    "npm-run-all": "^3.0.0",
    "surge": "^0.18.0",
    "swig": "^1.4.2",
    "yargs": "^5.0.0"
  }
}
