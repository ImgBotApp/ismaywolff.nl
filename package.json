{
  "author": "ismay",
  "description": "Source for ismaywolff.nl",
  "name": "ismaywolff.nl",
  "version": "0.1.0",
  "scripts": {
    "deploy": "npm-run-all test:* deploy:*",
    "deploy:build": "node build.js",
    "deploy:push": "./deploy.sh",
    "develop": "npm-run-all --parallel develop:*",
    "develop:serve": "browser-sync start --server '.tmp' --files '.tmp/**'",
    "develop:watch": "node build.js --development",
    "test": "npm-run-all test:*",
    "test:eslint": "eslint '{src,lib}/**/*.js'",
    "test:screenshot": "cd test && pageres '../build/index.html' '1280x800' '768x1024' '360x640' -c",
    "test:stylelint": "stylelint '{src,lib}/**/*.css'"
  },
  "devDependencies": {
    "autoprefixer": "^6.2.1",
    "browser-sync": "^2.8.2",
    "del": "^2.2.0",
    "eslint": "^2.2.0",
    "eslint-config-airbnb": "^6.0.2",
    "eslint-plugin-react": "^4.1.0",
    "glob": "^7.0.0",
    "metalsmith": "^2.0.1",
    "metalsmith-assets": "^0.1.0",
    "metalsmith-autoprefixer": "^1.1.0",
    "metalsmith-cloudinary": "^0.1.0",
    "metalsmith-collections": "^0.7.0",
    "metalsmith-define": "^1.0.0",
    "metalsmith-filenames": "^1.0.0",
    "metalsmith-fingerprint-ignore": "^1.1.2",
    "metalsmith-if": "0.1.1",
    "metalsmith-in-place": "^1.4.3",
    "metalsmith-mapsite": "^1.0.0",
    "metalsmith-postcss": "^3.1.0",
    "metalsmith-watch": "^1.0.1",
    "npm-run-all": "^1.5.1",
    "pageres": "^4.1.0",
    "pageres-cli": "^3.1.0",
    "postcss-calc": "^5.0.0",
    "postcss-custom-media": "^5.0.0",
    "postcss-custom-properties": "^5.0.0",
    "postcss-import": "^7.1.3",
    "stylelint": "^4.4.0",
    "stylelint-config-suitcss": "^4.0.0",
    "swig": "^1.4.2",
    "yargs": "^3.19.0"
  },
  "dependencies": {
    "suitcss-base": "^1.0.1",
    "suitcss-components-grid": "^3.0.1",
    "suitcss-utils": "^0.11.0"
  }
}
