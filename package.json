{
  "author": "ismay",
  "description": "Source for ismaywolff.nl",
  "name": "ismaywolff.nl",
  "version": "1.0.0",
  "scripts": {
    "build": "NODE_ENV=production webpack -p --bail",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "lint:js": "eslint client/**/*.{js,jsx}",
    "lint:css": "stylelint client/**/*.{js,jsx}",
    "lint:whitespace": "eclint check * client/** server/**",
    "start": "NODE_ENV=development webpack-dev-server --content-base dist/ --inline --hot",
    "test": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:ismay/ismaywolff.nl.git"
  },
  "greenkeeper": {
    "ignore": [
      "eslint-plugin-import",
      "eslint-plugin-react",
      "eslint-plugin-jsx-a11y"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}",
      "!client/index.jsx",
      "!client/{rootReducer,store}.js",
      "!client/{components,data,services}/*/index.js",
      "!client/services/analytics/**"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.2.0",
    "babel-loader": "^6.4.0",
    "babel-plugin-dynamic-import-node": "^1.0.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.2.1",
    "babel-preset-react": "^6.16.0",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^4.0.1",
    "coveralls": "^2.11.15",
    "css-loader": "^0.28.0",
    "dotenv": "^4.0.0",
    "eclint": "^2.0.1",
    "enzyme": "^2.6.0",
    "enzyme-to-json": "^1.4.4",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.24.1",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^19.0.2",
    "preload-webpack-plugin": "^1.2.2",
    "react-addons-test-utils": "^15.4.1",
    "style-loader": "^0.16.0",
    "stylelint": "^7.8.0",
    "stylelint-config-standard": "^16.0.0",
    "stylelint-processor-styled-components": "^0.1.0",
    "webpack": "^2.3.0",
    "webpack-dev-server": "^2.2.1"
  },
  "dependencies": {
    "date-fns": "^1.28.2",
    "history": "^4.6.1",
    "load-script": "^1.0.0",
    "normalize.css": "^6.0.0",
    "normalizr": "^3.0.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^5.0.2",
    "react-measure": "^1.4.6",
    "react-medium-image-zoom": "^0.9.0",
    "react-redux": "^5.0.2",
    "react-router": "^4.0.0",
    "react-router-dom": "^4.0.0",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "styled-components": "^1.3.1"
  }
}
